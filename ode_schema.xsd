			 <xsd:element name="Z" type="xsd:float"/>
		  </xsd:sequence>
		</xsd:extension>		  
	 </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="Quaternion">
	 <xsd:complexContent>
		<xsd:extension base="RSObject">
		  <xsd:sequence>
			 <xsd:element name="W" type="xsd:float"/>
			 <xsd:element name="I" type="xsd:float"/>
			 <xsd:element name="J" type="xsd:float"/>
